{
    "name": "slashfan/symfony-realworld-example-app",
    "description": "Exemplary real world application built with Symfony",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/rest-bundle": "^2.3",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5",
        "nesbot/carbon": "^2.8",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/cache": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.2",
        "symfony/property-access": "4.3.*",
        "symfony/property-info": "4.3.*",
        "symfony/security": "4.3.*",
        "symfony/serializer": "4.3.*",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "easycorp/easy-log-handler": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.11",
        "fzaninotto/faker": "^1.7",
        "geolid/phpcs": "^7.0",
        "kubawerlos/php-cs-fixer-custom-fixers": "^1.13",
        "nelmio/alice": "^3.2",
        "phpmd/phpmd": "2.6.*",
        "phpstan/phpstan-deprecation-rules": "^0.11.0",
        "phpstan/phpstan-doctrine": "^0.11.1",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-shim": "^0.11.2",
        "phpstan/phpstan-symfony": "^0.11.0",
        "phpunit/phpunit": "8.2.*",
        "povils/phpmnd": "^2.0",
        "sensiolabs/security-checker": "^5.0",
        "slam/phpstan-extensions": "^3.0",
        "squizlabs/php_codesniffer": "^3.3",
        "symfony/browser-kit": "4.3.*",
        "symfony/css-selector": "4.3.*",
        "symfony/debug-bundle": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/maker-bundle": "^1.3",
        "symfony/phpunit-bridge": "4.3.*",
        "symfony/profiler-pack": "1.0.*",
        "symfony/test-pack": "1.0.*",
        "symfony/var-dumper": "4.3.*",
        "symfony/web-server-bundle": "4.3.*",
        "thecodingmachine/phpstan-strict-rules": "^0.11.0",
        "theofidry/alice-data-fixtures": "^1.0"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "/src/Migrations/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}
