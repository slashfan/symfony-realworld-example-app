{
    "name": "slashfan/symfony-realworld-example-app",
    "type": "project",
    "description": "Exemplary real world application built with Symfony",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.1",
        "friendsofsymfony/rest-bundle": "^2.6",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/cors-bundle": "^2.0",
        "nesbot/carbon": "^2.27",
        "sensio/framework-extra-bundle": "^5.5",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/cache": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.4",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/security": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "easycorp/easy-log-handler": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "geolid/phpcs": "^7.0",
        "kubawerlos/php-cs-fixer-custom-fixers": "^1.16",
        "localheinz/composer-normalize": "^1.3",
        "nelmio/alice": "^3.5",
        "phpmd/phpmd": "2.6.*",
        "phpstan/phpstan-deprecation-rules": "^0.11.0",
        "phpstan/phpstan-doctrine": "^0.11.1",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "phpstan/phpstan-shim": "^0.11.2",
        "phpstan/phpstan-symfony": "^0.11.0",
        "phpunit/phpunit": "^8.4",
        "povils/phpmnd": "^2.1",
        "pyrech/composer-changelogs": "^1.7",
        "sensiolabs/security-checker": "^6.0",
        "slam/phpstan-extensions": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-bundle": "4.4.*",
        "symfony/maker-bundle": "^1.14",
        "symfony/phpunit-bridge": "4.4.*",
        "symfony/profiler-pack": "1.0.*",
        "symfony/test-pack": "1.0.*",
        "symfony/var-dumper": "4.4.*",
        "thecodingmachine/phpstan-strict-rules": "^0.11.0",
        "theofidry/alice-data-fixtures": "^1.1"
    },
    "config": {
        "platform": {
            "php": "7.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
